<odoo>
  <data>
    <!-- Inherit Form View to Modify it -->
    <record id="base_user_groups_view_cotaco" model="ir.ui.view">
        <field name="name">base.user.groups.view.cotaco</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.user_groups_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='in_group_78']" position="after">
                <field string="Aprueba Muestras?" name="aprueba_muestras"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>